          "update_interval": {
            "key": "Optional",
            "default": "1s"
          },
          "uart_id": {
            "key": "GeneratedID",
            "docs": "The uart Bus ID\n\n\n\n*See also: [PipSolar PV Inverter](https://esphome.io/components/pipsolar.html#configuration-variables)*"
          }
        },
        "extends": [
          "core.COMPONENT_SCHEMA"
        ]
      },
      "PIPSOLAR_COMPONENT_SCHEMA": {
        "config_vars": {
          "pipsolar_id": {
            "key": "Required"
          }
        }
      }
    }
