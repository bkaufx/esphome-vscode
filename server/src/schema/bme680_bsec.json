{
  "bme680_bsec": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "config_vars": {
          "id": {
            "key": "GeneratedID"
          },
          "temperature_offset": {
            "key": "Optional",
            "default": "0"
          },
          "iaq_mode": {
            "key": "Optional",
            "default": "STATIC",
            "type": "enum",
            "values": [
              "STATIC",
              "MOBILE"
            ]
          },
          "sample_rate": {
            "key": "Optional",
            "default": "LP",
            "type": "enum",
            "values": [
              "LP",
              "ULP"
            ]
          },
          "state_save_interval": {
            "key": "Optional",
            "default": "6hours",
            "type": "schema",
            "schema": {
              "extends": [
                "core.positive_time_period_minutes"
              ]
            }
          },
          "i2c_id": {
            "key": "GeneratedID"
          },
          "address": {
            "key": "Optional",
            "default": "118",
            "type": "schema",
            "schema": {
              "extends": [
                "core.hex_uint8_t"
              ]
            }
          }
        }
      }
    }
  },
