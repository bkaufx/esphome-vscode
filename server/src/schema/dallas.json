{
  "dallas": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "config_vars": {
          "id": {
            "key": "GeneratedID"
          },
          "pin": {
            "key": "Required",
            "schema": true,
            "internal": true,
            "modes": [
              "output"
            ],
            "type": "pin"
          },
          "update_interval": {
            "key": "Optional",
            "default": "60s"
          }
        },
        "extends": [
          "core.COMPONENT_SCHEMA"
        ]
      }
    }
  },
  "dallas.sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "extends": [
          "sensor.SENSOR_SCHEMA"
        ],
        "config_vars": {
          "id": {},
          "unit_of_measurement": {
            "default": "\u00b0C"
          },
          "accuracy_decimals": {
            "default": "1"
          },
          "device_class": {
            "default": "temperature"
          },
          "state_class": {
            "default": "measurement"
          },
          "dallas_id": {
            "key": "GeneratedID"
          },
          "address": {
            "key": "Optional"
          },
          "index": {
            "key": "Optional",
            "type": "integer"
          },
          "resolution": {
            "key": "Optional",
            "default": "12",
            "type": "integer"
          }
        }
      }
    }
  }
}