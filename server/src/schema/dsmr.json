  "dsmr": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "config_vars": {
          "id": {
            "key": "GeneratedID"
          },
          "decryption_key": {
            "key": "Optional"
          },
          "crc_check": {
            "key": "Optional",
            "default": "True",
            "type": "boolean"
          },
          "gas_mbus_id": {
            "key": "Optional",
            "default": "1",
            "type": "integer"
          },
          "max_telegram_length": {
            "key": "Optional",
            "default": "1500",
            "type": "integer"
          },
          "request_pin": {
            "key": "Optional",
            "schema": true,
            "modes": [
              "output"
            ],
            "type": "pin"
          },
          "request_interval": {
            "key": "Optional",
            "default": "0ms",
            "type": "schema",
            "schema": {
              "extends": [
                "core.positive_time_period_milliseconds"
              ]
            }
          },
          "receive_timeout": {
            "key": "Optional",
            "default": "200ms",
            "type": "schema",
            "schema": {
              "extends": [
                "core.positive_time_period_milliseconds"
              ]
            }
          },
          "uart_id": {
            "key": "GeneratedID"
          }
        }
      }
    }
  },
