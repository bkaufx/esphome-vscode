{
  "sensor": {
    "components": {
      "adc": {}
    },
    "schemas": {
      "validate_state_class": {
        "type": "enum",
        "values": [
          "",
          "measurement",
          "total_increasing"
        ]
      },
      "validate_filters": {
        "is_list": true,
        "type": "registry",
        "registry": "sensor.filter"
      },
      "validate_device_class": {
        "type": "enum",
        "values": [
          "",
          "aqi",
          "battery",
          "carbon_dioxide",
          "carbon_monoxide",
          "current",
          "energy",
          "gas",
          "humidity",
          "illuminance",
          "monetary",
          "nitrogen_dioxide",
          "nitrogen_monoxide",
          "nitrous_oxide",
          "ozone",
          "pm1",
          "pm10",
          "pm25",
          "power",
          "power_factor",
          "pressure",
          "signal_strength",
          "sulphur_dioxide",
          "temperature",
          "timestamp",
          "volatile_organic_compounds",
          "voltage"
        ]
      },
      "SENSOR_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "core.ENTITY_BASE_SCHEMA",
            "core.MQTT_COMPONENT_SCHEMA"
          ],
          "config_vars": {
            "mqtt_id": {
              "key": "Optional"
            },
            "id": {
              "key": "GeneratedID"
            },
            "unit_of_measurement": {
              "key": "Optional",
              "type": "string"
            },
            "accuracy_decimals": {
              "key": "Optional",
              "type": "integer"
            },
            "device_class": {
              "key": "Optional",
              "type": "enum",
              "values": [
                "",
                "aqi",
                "battery",
                "carbon_dioxide",
                "carbon_monoxide",
                "current",
                "energy",
                "gas",
                "humidity",
                "illuminance",
                "monetary",
                "nitrogen_dioxide",
                "nitrogen_monoxide",
                "nitrous_oxide",
                "ozone",
                "pm1",
                "pm10",
                "pm25",
                "power",
                "power_factor",
                "pressure",
                "signal_strength",
                "sulphur_dioxide",
                "temperature",
                "timestamp",
                "volatile_organic_compounds",
                "voltage"
              ]
            },
            "state_class": {
              "key": "Optional",
              "type": "enum",
              "values": [
                "",
                "measurement",
                "total_increasing"
              ]
            },
            "force_update": {
              "key": "Optional",
              "default": "False",
              "type": "boolean"
            },
            "expire_after": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.time_period_dict"
                ]
              }
            },
            "filters": {
              "key": "Optional",
              "type": "registry",
              "is_list": true,
              "registry": "sensor.filter"
            },
            "on_value": {
              "key": "Optional",
              "type": "trigger"
            },
            "on_raw_value": {
              "key": "Optional",
              "type": "trigger"
            },
            "on_value_range": {
              "key": "Optional",
              "type": "trigger",
              "schema": {
                "config_vars": {
                  "above": {
                    "key": "Optional"
                  },
                  "below": {
                    "key": "Optional"
                  },
                  "then": {
                    "type": "trigger"
                  }
                }
              },
              "has_required_var": true
            }
          }
        }
      },
      "QUANTILE_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "window_size": {
              "key": "Optional",
              "default": "5",
              "type": "integer"
            },
            "send_every": {
              "key": "Optional",
              "default": "5",
              "type": "integer"
            },
            "send_first_at": {
              "key": "Optional",
              "default": "1",
              "type": "integer"
            },
            "quantile": {
              "key": "Optional",
              "default": "0.9",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.zero_to_one_float"
                ]
              }
            }
          }
        }
      },
      "MEDIAN_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "window_size": {
              "key": "Optional",
              "default": "5",
              "type": "integer"
            },
            "send_every": {
              "key": "Optional",
              "default": "5",
              "type": "integer"
            },
            "send_first_at": {
              "key": "Optional",
              "default": "1",
              "type": "integer"
            }
          }
        }
      },
      "MIN_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "window_size": {
              "key": "Optional",
              "default": "5",
              "type": "integer"
            },
            "send_every": {
              "key": "Optional",
              "default": "5",
              "type": "integer"
            },
            "send_first_at": {
              "key": "Optional",
              "default": "1",
              "type": "integer"
            }
          }
        }
      },
      "MAX_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "window_size": {
              "key": "Optional",
              "default": "5",
              "type": "integer"
            },
            "send_every": {
              "key": "Optional",
              "default": "5",
              "type": "integer"
            },
            "send_first_at": {
              "key": "Optional",
              "default": "1",
              "type": "integer"
            }
          }
        }
      },
      "SLIDING_AVERAGE_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "window_size": {
              "key": "Optional",
              "default": "15",
              "type": "integer"
            },
            "send_every": {
              "key": "Optional",
              "default": "15",
              "type": "integer"
            },
            "send_first_at": {
              "key": "Optional",
              "default": "1",
              "type": "integer"
            }
          }
        }
      },
      "EXPONENTIAL_AVERAGE_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "alpha": {
              "key": "Optional",
              "default": "0.1",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_float"
                ]
              }
            },
            "send_every": {
              "key": "Optional",
              "default": "15",
              "type": "integer"
            },
            "send_first_at": {
              "key": "Optional",
              "default": "1",
              "type": "integer"
            }
          }
        }
      },
      "SENSOR_IN_RANGE_CONDITION_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required"
            },
            "above": {
              "key": "Optional"
            },
            "below": {
              "key": "Optional"
            }
          }
        }
      }
    },
    "condition": {
      "in_range": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.SENSOR_IN_RANGE_CONDITION_SCHEMA"
          ]
        }
      }
    },
    "filter": {
      "offset": {},
      "multiply": {},
      "filter_out": {},
      "quantile": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.QUANTILE_SCHEMA"
          ]
        }
      },
      "median": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.MEDIAN_SCHEMA"
          ]
        }
      },
      "min": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.MIN_SCHEMA"
          ]
        }
      },
      "max": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.MAX_SCHEMA"
          ]
        }
      },
      "sliding_window_moving_average": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.SLIDING_AVERAGE_SCHEMA"
          ]
        }
      },
      "exponential_moving_average": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.EXPONENTIAL_AVERAGE_SCHEMA"
          ]
        }
      },
      "throttle_average": {
        "type": "schema",
        "schema": {
          "extends": [
            "core.positive_time_period_milliseconds"
          ]
        }
      },
      "lambda": {},
      "delta": {},
      "or": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.validate_filters"
          ]
        }
      },
      "throttle": {
        "type": "schema",
        "schema": {
          "extends": [
            "core.positive_time_period_milliseconds"
          ]
        }
      },
      "heartbeat": {
        "type": "schema",
        "schema": {
          "extends": [
            "core.positive_time_period_milliseconds"
          ]
        }
      },
      "debounce": {
        "type": "schema",
        "schema": {
          "extends": [
            "core.positive_time_period_milliseconds"
          ]
        }
      },
      "calibrate_linear": {
        "is_list": true
      },
      "calibrate_polynomial": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "datapoints": {
              "key": "Required",
              "is_list": true
            },
            "degree": {
              "key": "Required",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_int"
                ]
              }
            }
          }
        }
      }
    }
  }
}