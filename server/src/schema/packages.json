{
  "packages": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "config_vars": {
          "<class 'str'>": {
            "type": "schema",
            "schema": {
              "extends": [
                "packages.BASE_SCHEMA"
              ]
            }
          }
        }
      },
      "BASE_SCHEMA": {
        "config_vars": {
          "url": {
            "key": "Required"
          },
          "username": {
            "key": "Optional",
            "type": "string"
          },
          "password": {
            "key": "Optional",
            "type": "string"
          },
          "file": {
            "key": "Optional"
          },
          "files": {
            "key": "Optional",
            "is_list": true
          },
          "ref": {
            "key": "Optional"
          },
          "refresh": {
            "key": "Optional",
            "default": "1d",
            "type": "string"
          }
        }
      }
    }
  }
}