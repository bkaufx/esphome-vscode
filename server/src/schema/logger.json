{
  "logger": {
    "action": {
      "log": {
        "is_list": true,
        "type": "schema",
        "schema": {
          "config_vars": {
            "format": {
              "key": "Required",
              "type": "string"
            },
            "args": {
              "key": "Optional",
              "default": "[]",
              "is_list": true
            },
            "level": {
              "key": "Optional",
              "default": "DEBUG",
              "type": "enum",
              "values": [
                "ERROR",
                "WARN",
                "INFO",
                "DEBUG",
                "VERBOSE",
                "VERY_VERBOSE"
              ]
            },
            "tag": {
              "key": "Optional",
              "default": "main",
              "type": "string"
            }
          }
        }
      }
    }
  }
}